{
  "name": "highway",
  "version": "0.1.2",
  "description": "Web sockets and HTTP access to mongo",
  "main": "highway.js",
  "scripts": {
    "test": "mocha test",
    "coverage": "mocha --require blanket --reporter mocha-lcov-reporter | ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krewenki/highway.git"
  },
  "config": {
    "blanket": {
      "pattern": [
        ""
      ],
      "data-cover-never": [
        "node_modules",
        "test"
      ]
    }
  },
  "keywords": [
    "websockets",
    "http",
    "mongodb"
  ],
  "author": "Warren Krewenki",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/krewenki/highway/issues"
  },
  "homepage": "https://github.com/krewenki/highway#readme",
  "dependencies": {
    "bcrypt": "^0.8.5",
    "body-parser": "^1.15.0",
    "connect-mongostore": "^0.1.4",
    "cookie-parser": "^1.4.1",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "mongodb": "^2.1.7",
    "mongojs": "^2.3.0",
    "nodemailer": "^2.3.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "passport.socketio": "^3.6.1",
    "promise": "^7.1.1",
    "socket.io": "^1.4.5",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "blanket": "^1.1.9",
    "expect": "^1.18.0",
    "mocha-lcov-reporter": "^1.2.0"
  }
}
